# Auth.js secret (set one of these)
AUTH_SECRET=
NEXTAUTH_SECRET=

# Neon Postgres (server-only)
DATABASE_URL=postgresql://<user>:<password>@<host>/<db>?sslmode=verify-full
# NEON_DATABASE_URL=postgresql://<user>:<password>@<host>/<db>?sslmode=verify-full

# One-time admin seed (run: pnpm db:seed-admin)
SEED_ADMIN_EMAIL=admin@example.com
SEED_ADMIN_PASSWORD=change-me
SEED_ADMIN_NAME=Initial Admin
SEED_ADMIN_ROLE=admin
SEED_ADMIN_USER_ID=admin@example.com
